      height: 40px; }
    .site-header .logo a {
      font-size: 32px;
      color: var(--text-color1); }
  .site-header .search-wrap {
    max-width: 50%;
    margin-left: auto;
    display: none; }
    @media (min-width: 992px) {
      .site-header .search-wrap {
