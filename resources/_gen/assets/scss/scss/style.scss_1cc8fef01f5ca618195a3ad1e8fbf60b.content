.toc-list {
  background-color: var(--card-background-color);
  border-radius: var(--card-radius);
  display: flex;
  flex-direction: column;
  overflow: hidden;
  padding-right: 30px;
  line-height: 2; }
  .toc-list a {
    color: var(--text-color1); }
  .toc-list ul, .toc-list li {
    color: var(--text-color1); }

