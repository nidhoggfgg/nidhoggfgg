.nav {
  display: flex;
  margin: 0; }
  .nav a {
    background: var(--card-background-color);
    border-radius: 0.5rem;
    padding-inline: 2rem;
    line-height: 3rem;
    color: var(--text-color1);
    text-decoration: none; }
  .nav .nav-prev {
